{
  "name": "uber-estimates-client",
  "description": "Client for the Uber Ride Estimates API",
  "version": "0.0.0-development",
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "files": [
    "build"
  ],
  "browser": "build/index.js",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.47",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/runtime": "^7.0.0-beta.47",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "@commitlint/prompt-cli": "^8.1.0",
    "axios-debug": "0.0.4",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "babel-preset-minify": "^0.4.0",
    "codecov": "^3.0.2",
    "dotenv": "^6.0.0",
    "es-check": "^2.0.3",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "rollup": "^0.66.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-babel-minify": "^6.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^4.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^3.0.0",
    "semantic-release": "^15.1.5",
    "travis-deploy-once": "^5.0.0"
  },
  "preferGlobal": false,
  "global": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/uber-estimates-client/tree/master"
  },
  "scripts": {
    "build": "BABEL_ENV=production rollup -c",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "gc": "commit",
    "is-es5": "es-check es5 build/index.js",
    "lint": "eslint --ext .js .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "test": "BABEL_ENV=test jest src/ --no-cache",
    "travis-deploy-once": "travis-deploy-once"
  }
}
